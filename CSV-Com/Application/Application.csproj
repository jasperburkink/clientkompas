<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Clients\Commands\DeleteClient\**" />
    <Compile Remove="Clients\Commands\UpdateClient\**" />
    <Compile Remove="Users\**" />
    <EmbeddedResource Remove="Clients\Commands\DeleteClient\**" />
    <EmbeddedResource Remove="Clients\Commands\UpdateClient\**" />
    <EmbeddedResource Remove="Users\**" />
    <None Remove="Clients\Commands\DeleteClient\**" />
    <None Remove="Clients\Commands\UpdateClient\**" />
    <None Remove="Users\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="Clients\Commands\UpdateClient\UpdateClientCommandValidator.cs" />
    <Compile Include="Clients\Commands\UpdateClient\UpdateClientCommand.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="MaritalStatuses\Commands\DeleteMaritalStatus\" />
    <Folder Include="MaritalStatuses\Commands\UpdateMaritalStatus\" />
    <Folder Include="MaritalStatuses\Queries\" />
    <Folder Include="Organizations\Commands\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Ardalis.GuardClauses" Version="4.3.0" />
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="11.0.0" />
    <PackageReference Include="FluentValidation" Version="11.7.1" />
    <PackageReference Include="FluentValidation.DependencyInjectionExtensions" Version="11.7.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.10" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Domain\Domain.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Common\Validators\Resources\Address.nl.Designer.cs">
      <DependentUpon>Address.nl.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Common\Validators\Resources\Address.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Address.resx</DependentUpon>
    </Compile>
    <Compile Update="Common\Validators\Resources\EmergencyPersonValidator.nl.Designer.cs">
      <DependentUpon>EmergencyPersonValidator.nl.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Common\Validators\Resources\EmergencyPersonValidator.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>EmergencyPersonValidator.resx</DependentUpon>
    </Compile>
    <Compile Update="Common\Validators\Resources\WorkingContractValidator.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>WorkingContractValidator.resx</DependentUpon>
    </Compile>
    <Compile Update="Common\Validators\Resources\WorkingContractValidator.nl.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>WorkingContractValidator.nl.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Common\Validators\Resources\Address.nl.resx">
      <LastGenOutput>Address.nl.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Common\Validators\Resources\Address.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Address.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Common\Validators\Resources\EmergencyPersonValidator.nl.resx">
      <LastGenOutput>EmergencyPersonValidator.nl.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Common\Validators\Resources\EmergencyPersonValidator.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>EmergencyPersonValidator.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Common\Validators\Resources\WorkingContractValidator.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>WorkingContractValidator.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Common\Validators\Resources\WorkingContractValidator.nl.resx">
      <Generator>ResXFileCodeGenerator</Generator>
